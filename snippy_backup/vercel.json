{
  "version": 2,
  "builds": [
    {
      "src": "snippy/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "snippy/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/snippy/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/snippy/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/snippy/index.py"
    }
  ]
}